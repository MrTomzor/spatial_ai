<launch>
    <!-- Load ROS parameters from the config file -->
    <rosparam command="load" file="$(find spatial_ai)/config/nav_tello.yaml"/>

    <arg name="venv" value="$(dirname)/../python-env/bin/python" />

    <!-- Run ov_extension.py node -->
    <node pkg="spatial_ai" type="ov_extension.py" name="ov_extension_node" output="screen" launch-prefix="$(arg venv)" />

    <!-- Run traj.py node -->
    <!-- <node pkg="spatial_ai" type="mrs_path_follower.py" name="traj_node" output="screen" /> -->
</launch>
